{
  "name": "word-search-kata",
  "version": "0.0.0",
  "description": "Word Search Kata Originally Written for ECMAScript 6.",
  "author": "Matt Morgan",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthewmorgan/word-search-kata.git"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.1",
    "jest-cli": "^20.0.4"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "package.json"
    ]
  },
  "scripts": {
    "test": "jest ./*",
    "watch": "jest --watch ./*",
    "watch:cover": "jest --coverage --watch ./*",
    "lint": "eslint .",
    "lint-test": "eslint . && jest --no-cache ./* "
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": "airbnb",
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": "off"
    }
  },
  "licenses": [
    "MIT"
  ],
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/matthewmorgan/word-search-kata/issues"
  },
  "homepage": "https://github.com/matthewmorgan/word-search-kata#readme",
  "main": "index.js",
  "license": "ISC"
}
